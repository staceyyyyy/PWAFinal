{
  "name": "swift-pwa",
  "version": "2.4.13",
  "private": true,
  "scripts": {
    "assets:build": "webpack",
    "local": "cross-env APP_ENV=local NODE_ENV=development node server.js",
    "local:start": "cross-env APP_ENV=local NODE_ENV=production node server.js",
    "dev": "cross-env APP_ENV=dev NODE_ENV=development node --max-http-header-size=16384 server.js",
    "dev:start": "cross-env APP_ENV=dev NODE_ENV=production node --max-http-header-size=16384 server.js",
    "stage": "cross-env APP_ENV=stage NODE_ENV=development node --max-http-header-size=16384 server.js",
    "stage:start": "cross-env APP_ENV=stage NODE_ENV=production node --max-http-header-size=16384 server.js",
    "prod": "cross-env APP_ENV=prod NODE_ENV=development node --max-http-header-size=16384 server.js",
    "prod:start": "cross-env APP_ENV=prod NODE_ENV=production node --max-http-header-size=16384 server.js",
    "start": "cross-env node --max-http-header-size=16384 server.js",
    "generate-certs": "mkdir certs && openssl req -x509 -days 365 -newkey rsa:2048 -nodes -sha256 -keyout certs/privateKey.key -out certs/certificate.crt",
    "build": "concurrently \"yarn assets:build\" \"next build\"",
    "export": "next export",
    "clean": "cross-env rimraf ./.next ./out",
    "release": "cross-env NODE_ENV=production npm run clean && npm run build && npm run export",
    "pretest": "./node_modules/.bin/eslint --ignore-path .gitignore . --fix"
  },
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@date-io/dayjs": "1.x",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.49",
    "@material-ui/pickers": "^3.2.10",
    "@paypal/checkout-server-sdk": "^1.0.2",
    "@paypal/react-paypal-js": "4.1.0",
    "@sentry/browser": "^5.24.2",
    "@sentry/integrations": "^5.24.2",
    "@sentry/node": "^5.24.2",
    "@sentry/tracing": "^5.24.2",
    "animate.css": "^4.1.1",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-link-error": "^1.1.13",
    "apollo-link-retry": "^2.2.16",
    "apollo-server-express": "^2.19.0",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cross-fetch": "^3.0.6",
    "dayjs": "^1.8.27",
    "disqus-react": "^1.0.11",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-user-agent-blocker": "^2.0.4",
    "file-selector": "^0.2.2",
    "firebase": "^8.2.7",
    "formik": "^2.1.4",
    "fscreen": "^1.2.0",
    "graphql": "^15.4.0",
    "graphql-request": "^1.8.2",
    "graphql-tools": "^7.0.1",
    "html-react-parser": "^1.2.4",
    "i18next": "^19.3.3",
    "isomorphic-unfetch": "^3.0.0",
    "jarallax": "^1.12.7",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "lru-cache": "^5.1.1",
    "next": "10.1.3",
    "next-i18next": "^7.0.1",
    "next-offline": "^5.0.0",
    "next-secure-headers": "^2.2.0",
    "open-iconic": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-countdown": "^2.3.2",
    "react-countdown-circle-timer": "^2.5.3",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.2.3",
    "react-facebook": "^8.1.4",
    "react-firebaseui": "^4.1.0",
    "react-google-maps": "^9.4.5",
    "react-google-recaptcha": "^2.1.0",
    "react-gtm-module": "^2.0.8",
    "react-prismazoom": "^1.1.4",
    "react-share": "^4.1.0",
    "react-slick": "^0.28.0",
    "simple-react-lightbox": "^3.6.6",
    "slick-carousel": "^1.8.1",
    "webfontloader": "^1.6.28",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@sentry/webpack-plugin": "^1.12.1",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "nodemon": "^2.0.3",
    "prettier": "2.0.4",
    "rimraf": "^3.0.2",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint --fix",
      "git add"
    ]
  }
}
